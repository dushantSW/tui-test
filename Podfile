target 'TUITest' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  target 'TUITestTests' do
    inherit! :search_paths

    # Pods for testing
    pod 'Fakery'
    pod 'Nimble'
  end

end
